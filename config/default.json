{
    "default_model": "td2",
    "models": {
        "td2": {
            "type": "AZURE-OAI",
            "url": "https://archipelago-openai-eu.openai.azure.com/openai/deployments/archipelago-davinci/completions?api-version=2022-06-01-preview",
            "headers": {
                "api-key": "{{AZURE_OAI_API_KEY}}",
                "Content-Type": "application/json"
            },
            "requestsPerSecond": 20,
            "maxChunkLength": 2000
        },
        "td3": {
            "type": "OPENAI",
            "url": "https://api.openai.com/v1/completions",
            "headers": {
                "Authorization": "Bearer {{OPENAI_API_KEY}}",
                "Content-Type": "application/json"
            },
            "params": {
                "model": "text-davinci-003"
            },
            "requestsPerSecond": 10,
            "maxChunkLength": 2000
        }
    },
    "cache": false,
    "context": {},
    "server" : "standalone"
}